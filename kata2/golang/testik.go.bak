package main

import "fmt"

var input string = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz"
var output string = "NOPQRSTUVWXYZABCDEFGHIJKLMnopqrstuvwxyzabcdefghijklm"

func main() {
    for pos, val := range input {
        fmt.Println(val, string(val), " equals to ", output[pos], string(output[pos]))
    }
}
